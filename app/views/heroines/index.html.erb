<h2>All Heroines</h2>

<ul>
  <% @heroines.each do |heroine| %>
    <li>
      <%= link_to heroine.super_name, heroine_path(heroine) %>
    </li>
    <br>
  <% end %>
</ul>

<p><%= link_to "See all powers", powers_path %></p>

<%= form_with(url: "/heroines/search", method: "get") do %>
  <%= label_tag(:q, "Search by power:") %>
  <%= text_field_tag(:q) %>
  <%= submit_tag("Search") %>
<% end %>