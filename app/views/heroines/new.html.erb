<h1> Create a new Heroine </h1>

<% @heroine.errors.full_messages.each do |fm| %>
    <%= fm %>
    <% end %>

<div>
<%= form_for @heroine do |f| %>

    <%= f.label :name %>
    <%= f.text_field :name %> 
    <br>

    <%= f.label :super_name %>
    <%= f.text_field :super_name %> 
    <br>

    <%= f.submit %> 
<% end %> 

</div>